							{
								"referenceName": "DS_ABLB_SI_CharityComissionDatabase_unzip_1",
								"type": "DatasetReference",
								"parameters": {
									"filename": {
										"value": "@pipeline().parameters.relativeURL",
										"type": "Expression"
									}
								}
							}
						]
					}
				],
