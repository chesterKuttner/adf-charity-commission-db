		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-build-base-data"
		},
		"LS_ABLB_ADF_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_ADF'"
